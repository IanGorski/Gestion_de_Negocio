{
  "name": "sistema-gestion-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 4173",
    "analyze": "start dist/stats.html",
    "lh:run": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless=new\" --view",
    "lh:once": "wait-on http://localhost:4173 && lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless=new\" --view",
    "lh:preview": "concurrently -k -s first -n server,audit -c green,blue \"vite preview --port 4173\" \"npm run lh:once\"",
    "lh": "npm run build && npm run lh:preview",
    "optimize:images": "node scripts/optimize-images.mjs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.4",
    "axios": "^1.13.1",
    "date-fns": "^4.1.0",
    "driver.js": "^1.3.6",
    "framer-motion": "^12.23.24",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "papaparse": "^5.4.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "react-toastify": "^11.0.5",
    "react-window": "^2.2.2",
    "recharts": "^3.3.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "lighthouse": "^12.8.2",
    "postcss": "^8.5.6",
    "react-virtualized-auto-sizer": "^1.0.26",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.7",
    "vite-plugin-compression": "^0.5.1",
    "wait-on": "^8.0.5"
  }
}
